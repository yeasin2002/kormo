{
  "name": "tsdown-starter",
  "version": "0.0.0",
  "private": true,
  "description": "A simple example library built by tsdown",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
			"build": "tsdown",
			"dev": "tsdown --watch",
			"openapi": "pnpm openapi-typescript http://localhost:4000/docs-json -o packages/api-types/index.ts"
		},
  "devDependencies": {
			"tsdown": "latest"
		},
		"dependencies": {
			"openapi-typescript": "^7.9.1"
		}
}
